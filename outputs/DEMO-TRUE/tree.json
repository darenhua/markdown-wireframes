{
  "root": "welcome-container",
  "elements": {
    "welcome-container": {
      "key": "welcome-container",
      "type": "Box",
      "props": {
        "padding": "lg",
        "align": "center"
      },
      "children": [
        "welcome-card"
      ]
    },
    "welcome-card": {
      "key": "welcome-card",
      "type": "Card",
      "props": {
        "title": "Welcome!",
        "description": "We're glad you're here!"
      },
      "children": [
        "card-content"
      ]
    },
    "card-content": {
      "key": "card-content",
      "type": "Stack",
      "props": {
        "direction": "vertical",
        "gap": "md",
        "align": "center"
      },
      "children": [
        "greeting-text",
        "get-started-btn"
      ]
    },
    "greeting-text": {
      "key": "greeting-text",
      "type": "Text",
      "props": {
        "text": "We're excited to have you here. Let's get you started on your journey!",
        "size": "base"
      },
      "children": []
    },
    "get-started-btn": {
      "key": "get-started-btn",
      "type": "Button",
      "props": {
        "label": "Get Started",
        "variant": "default",
        "size": "default"
      },
      "children": []
    }
  }
}
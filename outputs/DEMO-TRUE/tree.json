{
  "root": "dashboard-root",
  "elements": {
    "dashboard-root": {
      "key": "dashboard-root",
      "type": "Box",
      "props": {
        "padding": "lg"
      },
      "children": [
        "dashboard-header",
        "dashboard-content"
      ]
    },
    "dashboard-header": {
      "key": "dashboard-header",
      "type": "Stack",
      "props": {
        "direction": "horizontal",
        "align": "center",
        "gap": "md"
      },
      "children": [
        "dashboard-title",
        "header-actions"
      ]
    },
    "dashboard-title": {
      "key": "dashboard-title",
      "type": "Heading",
      "props": {
        "text": "Social Media Dashboard",
        "level": "1"
      },
      "children": []
    },
    "header-actions": {
      "key": "header-actions",
      "type": "Stack",
      "props": {
        "direction": "horizontal",
        "gap": "sm"
      },
      "children": [
        "refresh-btn",
        "settings-btn"
      ]
    },
    "refresh-btn": {
      "key": "refresh-btn",
      "type": "Button",
      "props": {
        "label": "Refresh",
        "variant": "secondary"
      },
      "children": []
    },
    "settings-btn": {
      "key": "settings-btn",
      "type": "Button",
      "props": {
        "label": "Settings",
        "variant": "outline"
      },
      "children": []
    },
    "dashboard-content": {
      "key": "dashboard-content",
      "type": "Stack",
      "props": {
        "direction": "vertical",
        "gap": "lg"
      },
      "children": [
        "metrics-section",
        "main-grid"
      ]
    },
    "metrics-section": {
      "key": "metrics-section",
      "type": "Card",
      "props": {
        "title": "Performance Metrics"
      },
      "children": [
        "metrics-grid"
      ]
    },
    "metrics-grid": {
      "key": "metrics-grid",
      "type": "Grid",
      "props": {
        "columns": 4,
        "gap": "md"
      },
      "children": [
        "metric-followers",
        "metric-engagement",
        "metric-reach",
        "metric-impressions"
      ]
    },
    "metric-followers": {
      "key": "metric-followers",
      "type": "Metric",
      "props": {
        "label": "Total Followers",
        "value": "24,580",
        "change": "+12%",
        "trend": "up"
      },
      "children": []
    },
    "metric-engagement": {
      "key": "metric-engagement",
      "type": "Metric",
      "props": {
        "label": "Engagement Rate",
        "value": "4.8%",
        "change": "+0.5%",
        "trend": "up"
      },
      "children": []
    },
    "metric-reach": {
      "key": "metric-reach",
      "type": "Metric",
      "props": {
        "label": "Total Reach",
        "value": "156,200",
        "change": "+8%",
        "trend": "up"
      },
      "children": []
    },
    "metric-impressions": {
      "key": "metric-impressions",
      "type": "Metric",
      "props": {
        "label": "Impressions",
        "value": "342,100",
        "change": "-2%",
        "trend": "down"
      },
      "children": []
    },
    "main-grid": {
      "key": "main-grid",
      "type": "Grid",
      "props": {
        "columns": 3,
        "gap": "lg"
      },
      "children": [
        "profile-card",
        "insights-card",
        "engagement-card",
        "feed-card",
        "notifications-card",
        "platforms-tabs"
      ]
    },
    "profile-card": {
      "key": "profile-card",
      "type": "Card",
      "props": {
        "title": "Profile"
      },
      "children": [
        "profile-stack"
      ]
    },
    "profile-stack": {
      "key": "profile-stack",
      "type": "Stack",
      "props": {
        "direction": "vertical",
        "gap": "md",
        "align": "center"
      },
      "children": [
        "avatar",
        "username"
      ]
    },
    "avatar": {
      "key": "avatar",
      "type": "Avatar",
      "props": {
        "src": "https://example.com/avatar.jpg",
        "fallback": "U",
        "alt": "User Avatar"
      },
      "children": []
    },
    "username": {
      "key": "username",
      "type": "Heading",
      "props": {
        "text": "John Doe",
        "level": "2"
      },
      "children": []
    },
    "insights-card": {
      "key": "insights-card",
      "type": "Card",
      "props": {
        "title": "Key Insights"
      },
      "children": [
        "insights-stack"
      ]
    },
    "insights-stack": {
      "key": "insights-stack",
      "type": "Stack",
      "props": {
        "direction": "vertical",
        "gap": "md"
      },
      "children": [
        "followers-metric",
        "likes-metric",
        "comments-metric"
      ]
    },
    "followers-metric": {
      "key": "followers-metric",
      "type": "Metric",
      "props": {
        "label": "Followers",
        "value": "10.5k",
        "change": "+12%",
        "trend": "up"
      },
      "children": []
    },
    "likes-metric": {
      "key": "likes-metric",
      "type": "Metric",
      "props": {
        "label": "Likes",
        "value": "23.1k",
        "change": "+5%",
        "trend": "up"
      },
      "children": []
    },
    "comments-metric": {
      "key": "comments-metric",
      "type": "Metric",
      "props": {
        "label": "Comments",
        "value": "5.2k",
        "change": "+8%",
        "trend": "up"
      },
      "children": []
    },
    "engagement-card": {
      "key": "engagement-card",
      "type": "Card",
      "props": {
        "title": "Engagement"
      },
      "children": [
        "engagement-stack"
      ]
    },
    "engagement-stack": {
      "key": "engagement-stack",
      "type": "Stack",
      "props": {
        "direction": "vertical",
        "gap": "md"
      },
      "children": [
        "shares-metric",
        "saves-metric",
        "audience-text"
      ]
    },
    "shares-metric": {
      "key": "shares-metric",
      "type": "Metric",
      "props": {
        "label": "Shares",
        "value": "2.8k",
        "change": "+2%",
        "trend": "up"
      },
      "children": []
    },
    "saves-metric": {
      "key": "saves-metric",
      "type": "Metric",
      "props": {
        "label": "Saves",
        "value": "1.5k",
        "change": "-1%",
        "trend": "down"
      },
      "children": []
    },
    "audience-text": {
      "key": "audience-text",
      "type": "Text",
      "props": {
        "text": "Age: 18-34, 60% Female"
      },
      "children": []
    },
    "feed-card": {
      "key": "feed-card",
      "type": "Card",
      "props": {
        "title": "Recent Posts"
      },
      "children": [
        "feed-stack"
      ]
    },
    "feed-stack": {
      "key": "feed-stack",
      "type": "Stack",
      "props": {
        "direction": "vertical",
        "gap": "md"
      },
      "children": [
        "post-1",
        "post-2",
        "post-3"
      ]
    },
    "post-1": {
      "key": "post-1",
      "type": "Text",
      "props": {
        "text": "Post 1: Image of sunset",
        "variant": "default",
        "size": "base"
      },
      "children": []
    },
    "post-2": {
      "key": "post-2",
      "type": "Text",
      "props": {
        "text": "Post 2: Announcement of new product",
        "variant": "default",
        "size": "base"
      },
      "children": []
    },
    "post-3": {
      "key": "post-3",
      "type": "Text",
      "props": {
        "text": "Post 3: Question about favorite features",
        "variant": "default",
        "size": "base"
      },
      "children": []
    },
    "notifications-card": {
      "key": "notifications-card",
      "type": "Card",
      "props": {
        "title": "Notifications"
      },
      "children": [
        "notifications-list"
      ]
    },
    "notifications-list": {
      "key": "notifications-list",
      "type": "List",
      "props": {
        "items": [
          "User4 liked your post",
          "User5 started following you"
        ],
        "ordered": false
      },
      "children": []
    },
    "platforms-tabs": {
      "key": "platforms-tabs",
      "type": "Tabs",
      "props": {
        "defaultValue": "twitter"
      },
      "children": [
        "platforms-list",
        "twitter-content",
        "instagram-content",
        "linkedin-content"
      ]
    },
    "platforms-list": {
      "key": "platforms-list",
      "type": "TabsList",
      "props": {},
      "children": [
        "twitter-trigger",
        "instagram-trigger",
        "linkedin-trigger"
      ]
    },
    "twitter-trigger": {
      "key": "twitter-trigger",
      "type": "TabsTrigger",
      "props": {
        "value": "twitter",
        "label": "Twitter"
      },
      "children": []
    },
    "instagram-trigger": {
      "key": "instagram-trigger",
      "type": "TabsTrigger",
      "props": {
        "value": "instagram",
        "label": "Instagram"
      },
      "children": []
    },
    "linkedin-trigger": {
      "key": "linkedin-trigger",
      "type": "TabsTrigger",
      "props": {
        "value": "linkedin",
        "label": "LinkedIn"
      },
      "children": []
    },
    "twitter-content": {
      "key": "twitter-content",
      "type": "TabsContent",
      "props": {
        "value": "twitter"
      },
      "children": [
        "twitter-stats"
      ]
    },
    "twitter-stats": {
      "key": "twitter-stats",
      "type": "Stack",
      "props": {
        "direction": "vertical",
        "gap": "md"
      },
      "children": [
        "twitter-metric1",
        "twitter-metric2"
      ]
    },
    "twitter-metric1": {
      "key": "twitter-metric1",
      "type": "Metric",
      "props": {
        "label": "Tweets",
        "value": "342",
        "change": "+15",
        "trend": "up"
      },
      "children": []
    },
    "twitter-metric2": {
      "key": "twitter-metric2",
      "type": "Metric",
      "props": {
        "label": "Retweets",
        "value": "1,245",
        "change": "+89",
        "trend": "up"
      },
      "children": []
    },
    "instagram-content": {
      "key": "instagram-content",
      "type": "TabsContent",
      "props": {
        "value": "instagram"
      },
      "children": [
        "instagram-stats"
      ]
    },
    "instagram-stats": {
      "key": "instagram-stats",
      "type": "Stack",
      "props": {
        "direction": "vertical",
        "gap": "md"
      },
      "children": [
        "instagram-metric1",
        "instagram-metric2"
      ]
    },
    "instagram-metric1": {
      "key": "instagram-metric1",
      "type": "Metric",
      "props": {
        "label": "Posts",
        "value": "128",
        "change": "+8",
        "trend": "up"
      },
      "children": []
    },
    "instagram-metric2": {
      "key": "instagram-metric2",
      "type": "Metric",
      "props": {
        "label": "Likes",
        "value": "12,450",
        "change": "+890",
        "trend": "up"
      },
      "children": []
    },
    "linkedin-content": {
      "key": "linkedin-content",
      "type": "TabsContent",
      "props": {
        "value": "linkedin"
      },
      "children": [
        "linkedin-stats"
      ]
    },
    "linkedin-stats": {
      "key": "linkedin-stats",
      "type": "Stack",
      "props": {
        "direction": "vertical",
        "gap": "md"
      },
      "children": [
        "linkedin-metric1",
        "linkedin-metric2"
      ]
    },
    "linkedin-metric1": {
      "key": "linkedin-metric1",
      "type": "Metric",
      "props": {
        "label": "Posts",
        "value": "45",
        "change": "+3",
        "trend": "up"
      },
      "children": []
    },
    "linkedin-metric2": {
      "key": "linkedin-metric2",
      "type": "Metric",
      "props": {
        "label": "Engagements",
        "value": "892",
        "change": "+156",
        "trend": "up"
      },
      "children": []
    }
  }
}
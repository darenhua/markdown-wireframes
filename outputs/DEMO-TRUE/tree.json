{
  "root": "dashboard-root",
  "elements": {
    "dashboard-root": {
      "key": "dashboard-root",
      "type": "Card",
      "props": {
        "title": "Metrics Dashboard",
        "description": "Key performance indicators and growth statistics"
      },
      "children": [
        "dashboard-container"
      ]
    },
    "dashboard-container": {
      "key": "dashboard-container",
      "type": "Stack",
      "props": {
        "direction": "vertical",
        "gap": "lg"
      },
      "children": [
        "stats-grid",
        "growth-section"
      ]
    },
    "stats-grid": {
      "key": "stats-grid",
      "type": "Grid",
      "props": {
        "columns": 3,
        "gap": "md"
      },
      "children": [
        "revenue-metric",
        "users-metric",
        "conversion-metric"
      ]
    },
    "revenue-metric": {
      "key": "revenue-metric",
      "type": "Box",
      "props": {
        "padding": "md",
        "rounded": "md",
        "border": true,
        "shadow": "sm"
      },
      "children": [
        "revenue-label",
        "revenue-value",
        "revenue-change"
      ]
    },
    "revenue-label": {
      "key": "revenue-label",
      "type": "Text",
      "props": {
        "text": "Total Revenue",
        "variant": "muted",
        "size": "sm"
      },
      "children": []
    },
    "revenue-value": {
      "key": "revenue-value",
      "type": "Heading",
      "props": {
        "text": "$124,580",
        "level": "2"
      },
      "children": []
    },
    "revenue-change": {
      "key": "revenue-change",
      "type": "Stack",
      "props": {
        "direction": "horizontal",
        "gap": "sm",
        "align": "center"
      },
      "children": [
        "revenue-trend-icon",
        "revenue-trend-text"
      ]
    },
    "revenue-trend-icon": {
      "key": "revenue-trend-icon",
      "type": "Icon",
      "props": {
        "name": "arrow-right",
        "size": "sm"
      },
      "children": []
    },
    "revenue-trend-text": {
      "key": "revenue-trend-text",
      "type": "Text",
      "props": {
        "text": "+12.5% from last month",

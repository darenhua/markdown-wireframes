{
  "root": "welcome-card",
  "elements": {
    "welcome-card": {
      "key": "welcome-card",
      "type": "Card",
      "props": {
        "title": "Welcome",
        "bg": "gradient-warm"
      },
      "children": [
        "title-heart-icon",
        "heart-icon",
        "greeting-text",
        "get-started-btn",
        "secondary-btn",
        "info-section",
        "contact-section",
        "features-section",
        "testimonials-section"
      ]
    },
    "greeting-text": {
      "key": "greeting-text",
      "type": "Text",
      "props": {
        "text": "Hello! Welcome to our app."
      }
    },
    "get-started-btn": {
      "key": "get-started-btn",
      "type": "Button",
      "props": {
        "label": "Get Started",
        "variant": "primary"
      }
    },
    "heart-icon": {
      "key": "heart-icon",
      "type": "Icon",
      "props": {
        "name": "heart",
        "size": "md",
        "color": "primary"
      }
    },
    "secondary-btn": {
      "key": "secondary-btn",
      "type": "Button",
      "props": {
        "label": "Secondary Action",
        "variant": "outline"
      }
    },
    "info-section": {
      "key": "info-section",
      "type": "Card",
      "props": {
        "title": "Information",
        "bg": "muted"
      },
      "children": [
        "info-text"
      ]
    },
    "info-text": {
      "key": "info-text",
      "type": "Text",
      "props": {
        "text": "Here's some additional information about our app.",
        "variant": "muted"
      }
    },
    "contact-section": {
      "key": "contact-section",
      "type": "Card",
      "props": {
        "title": "Contact Us",
        "bg": "purple"
      },
      "children": [
        "contact-text",
        "contact-btn"
      ]
    },
    "contact-text": {
      "key": "contact-text",
      "type": "Text",
      "props": {
        "text": "Reach out to us anytime for support."
      }
    },
    "contact-btn": {
      "key": "contact-btn",
      "type": "Button",
      "props": {
        "label": "Contact Support",
        "variant": "secondary"
      }
    },
    "features-section": {
      "key": "features-section",
      "type": "Card",
      "props": {
        "title": "Features",
        "bg": "gradient-cool"
      },
      "children": [
        "features-text"
      ]
    },
    "features-text": {
      "key": "features-text",
      "type": "Text",
      "props": {
        "text": "Explore the amazing features we offer."
      }
    },
    "testimonials-section": {
      "key": "testimonials-section",
      "type": "Card",
      "props": {
        "title": "Testimonials",
        "bg": "gradient-warm"
      },
      "children": [
        "testimonials-text"
      ]
    },
    "testimonials-text": {
      "key": "testimonials-text",
      "type": "Text",
      "props": {
        "text": "See what our users are saying about us."
      }
    },
    "title-heart-icon": {
      "key": "title-heart-icon",
      "type": "Icon",
      "props": {
        "name": "heart",
        "size": "md",
        "color": "pink"
      }
    }
  }
}
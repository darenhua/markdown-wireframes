{
  "root": "metrics-dashboard",
  "elements": {
    "metrics-dashboard": {
      "key": "metrics-dashboard",
      "type": "Grid",
      "props": {
        "columns": 3,
        "gap": "lg"
      },
      "children": [
        "revenue-metric",
        "users-metric",
        "growth-metric"
      ]
    },
    "revenue-metric": {
      "key": "revenue-metric",
      "type": "Metric",
      "props": {
        "label": "Revenue",
        "value": "$120,000",
        "change": "+5%",
        "trend": "up"
      }
    },
    "users-metric": {
      "key": "users-metric",
      "type": "Metric",
      "props": {
        "label": "Users",
        "value": "8,500",
        "change": "+2%",
        "trend": "up"
      }
    },
    "growth-metric": {
      "key": "growth-metric",
      "type": "Metric",
      "props": {
        "label": "Growth",
        "value": "15%",
        "change": "+3%",
        "trend": "up"
      }
    }
  }
}